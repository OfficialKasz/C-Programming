#include <stdio.h>
#include <stdlib.h>


int main() {
	int var1, var2, answer;
	var1=23;
	var2=14;
	answer = var1 & var2;
	printf("%d & %d = %d\n", var1, var2, answer);
	
	answer = var1 | var2;
	printf("%d | %d = %d\n", var1, var2, answer);
	
	answer = var1 ^ var2;
	printf("%d ^ %d = %d\n", var1, var2, answer);
	
	answer = -var1;
	printf("-%d = %d\n", var1, answer);
	
	answer = var1 << 2;
	printf("%d << 2 = %d\n", var1, answer);
	
	answer = var1 >> 2;
	printf("%d >> 2 = %d\n", var1, answer);
	
	return 0;
}
